	};


	--// Flags_Camera
	WillDetectCamera = { -- [ "true" or "false" = Fetch_Method_Global(Classic) / Detected / NaN ] --

		DaHood = true;
		DaHoodVC = true;
		HoodModded = true;
		HoodModdedVC = true;
		HoodDuels = true;
		HoodArena = true;
		TheHood = true;
		UntitledHood = true;
		HoodCustoms = true;
		HoodAimTrainer = true;
		HoodAimTrainer2 = true;
		Dat = true;
		DatHood = true;
		DatHoodOG = true;
		DahHood = true;
		DahHood2 = true;
		DahHood3 = true;
		DaHoodTrainer = true;
		DaHoodAimTrainer = true;
		FiveDuels = true;
		DahAimTrainer = true;
		DaDownhill = true;
		
		CombatWarriors = true;
		CombatWarriorsBEGINNERS = true;

		SaitamaGame = true;

		PhantomForces = false;
		
		HoodWorldTESTPLACE = true;
		
		UknownGame = false;

	};


	--// Flags_Silent
	WillDetectSilent = { -- [ "true" = Fetch_Method_Select(LoadBypass, Spoof, MouseProperties) / Detected / NaN ] --

		DaHood = true;
		DaHoodVC = true;
		HoodModded = false;
		HoodModdedVC = false;
