		TriggerBotButton.BackgroundColor3 = Color3.new(1, 1, 1)
	end
end)
FOV_VISIBLEButton.MouseButton1Down:Connect(function()
	if TweakFOV.FOV_VisibleOrNot == false then
		TweakFOV.FOV_VisibleOrNot = true
		FOV_VISIBLEButton.BackgroundColor3 = Color3.new(1, 0, 0)
		FOV_CIRCLE.Filled = false
		FOV_CIRCLE.Visible = true
		CircleMouse()
	elseif TweakFOV.FOV_VisibleOrNot == true then
		TweakFOV.FOV_VisibleOrNot = false
		FOV_CIRCLE.Visible = false
		FOV_CIRCLE.Filled = false
		FOV_CIRCLE.Color = TweakFOV.FOV_Color
		FOV_VISIBLEButton.BackgroundColor3 = Color3.new(1, 1, 1)
	end
end)

CamBindBox:GetPropertyChangedSignal("Text"):Connect(function()
	CamBindBox.Text = CamBindBox.Text:sub(1,1):upper()
end)
MouseBindBox:GetPropertyChangedSignal("Text"):Connect(function()
	MouseBindBox.Text = MouseBindBox.Text:sub(1,1):upper()
end)
SilentBindBox:GetPropertyChangedSignal("Text"):Connect(function()
	SilentBindBox.Text = SilentBindBox.Text:sub(1,1):upper()
end)
local VerifyIntRadius
RadiusBox:GetPropertyChangedSignal("Text"):Connect(function()
	local Int = RadiusBox.Text
	if Int == "" then
		VerifyIntRadius = nil
	elseif tonumber(Int) then
		VerifyIntRadius = math.floor(Int)
	end
	RadiusBox.Text = VerifyIntRadius or ""
	FOV_CIRCLE.Radius = tonumber(RadiusBox.Text)
end)


