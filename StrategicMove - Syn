--\\ Drawing Circle //--
local FOV_CIRCLE = Drawing.new("Circle")
FOV_CIRCLE.Filled = false
FOV_CIRCLE.Thickness = 1
FOV_CIRCLE.Transparency = 1
FOV_CIRCLE.Color = TweakFOV.FOV_Color
FOV_CIRCLE.Radius = TweakFOV.FOV_RADIUS
FOV_CIRCLE.Visible = TweakFOV.FOV_VisibleOrNot
FOV_CIRCLE.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)


--\\ Tab Functions //--
AimBot_Button.MouseButton1Down:Connect(function()
	RAGE_FRAME1.Visible = false
	RAGE_FRAME2.Visible = false
	VISUALS_FRAME.Visible = false
	VISUALS_FRAME2.Visible = false
	AIMBOT_FRAME1.Visible = true
	AIMBOT_FRAME2.Visible = true
end)
Rage_Button.MouseButton1Down:Connect(function()
	VISUALS_FRAME.Visible = false
	VISUALS_FRAME2.Visible = false
	AIMBOT_FRAME1.Visible = false
	AIMBOT_FRAME2.Visible = false
	RAGE_FRAME1.Visible = true
	RAGE_FRAME2.Visible = true
end)
ESP_Button.MouseButton1Down:Connect(function()
	RAGE_FRAME1.Visible = false
	RAGE_FRAME2.Visible = false
	AIMBOT_FRAME1.Visible = false
	AIMBOT_FRAME2.Visible = false
	VISUALS_FRAME.Visible = true
	VISUALS_FRAME2.Visible = true
end)


--\\ Sound //--
local function HeartAche()
	local Ping = Instance.new("Sound", SoundContainer)
	Ping.Name = "Sound";
	Ping.Volume = 2.5;
	Ping.TimePosition = 0;
	Ping.Looped = false;
	Ping.SoundId = "rbxassetid://187338447";
	Ping.PlaybackSpeed = 1.1;
	Ping:Play();
end
local function Welcome()
	local Ding = Instance.new("Sound", SoundContainer)
	Ding.Name = "Sound";
	Ding.Volume = 3;
	Ding.TimePosition = 0;
	Ding.Looped = false;
	Ding.SoundId = "rbxassetid://3398620867";
	Ding.PlaybackSpeed = 1;
	Ding:Play();
end
local function Dismay()
	iCandy.Visible = true
	HeartAche()
	task.wait(3.2)
	iCandy.Visible = false
	for _, v in pairs(SoundContainer:GetChildren()) do
		pcall(function()
			if v:IsA("Sound") then task.wait(.5)
				v.Parent = SoundContainer task.wait()
				SoundContainer:ClearAllChildren()
			end
		end)
	end
end


--\\ Zombie/Tryhard Animation //--
local function TryHardAnimation()
	local DayZ = LocalPlayer.Character:FindFirstChild("Animate")
	DayZ.idle.Animation1.AnimationId = "http://www.roblox.com/asset/?id=616158929"
	DayZ.idle.Animation2.AnimationId = "http://www.roblox.com/asset/?id=616160636"
	DayZ.walk.WalkAnim.AnimationId = "http://www.roblox.com/asset/?id=616168032"
	DayZ.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=616163682"
	DayZ.fall.FallAnim.AnimationId = "http://www.roblox.com/asset/?id=707829716"
end 


--\\ TeamChecker //--
local SearchTeamBased = function()
	local First = false
	local Second = false
	if LocalPlayer.Team ~= nil and LocalPlayer.Neutral == false then
		if #Teams:GetTeams() > 0 then
			First = true
			for _, v in pairs(Teams:GetTeams()) do
				if #v:GetPlayers() > 0 and v ~= LocalPlayer.Team and First == true then
					Second = true
				elseif #v:GetPlayers() <= 0 and First == true then
					return "FFA"
				end
			end
		elseif Teams:GetTeams() <= 0 then
			return "FFA"
		end
	elseif LocalPlayer.Neutral == true then
		return "FFA"	
	elseif LocalPlayer.Team == nil then
		return "FFA"
	end
	if First == true and Second == true then
		return "TEAMS"
	end
end


--\\ BackTrack //--
local function BackTrackAll()
	canmake = true
	local EnemiesFFA = nil
	local EnemiesTEAM = nil
	local Target = nil
	if canmake then
		pcall(function()

			local TeamBasedOrNot = SearchTeamBased()

			local ReturnStates = false
			if TeamBasedOrNot == "TEAMS" then
				ReturnStates = true
			elseif TeamBasedOrNot == "FFA" then
				ReturnStates = false
			end

			if ReturnStates == false then
				for _,v in pairs(Players:GetPlayers()) do
					if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 then

						EnemiesFFA = v.Character

						for _,v in pairs(EnemiesFFA:GetChildren()) do
							if v:IsA("BasePart") then
								spawn(function()

									local Shadows = Instance.new("Part", ShadowHolder)
									local ChamsOnEm = Instance.new("BoxHandleAdornment", ShadowHolder)
									local PlaceHolderHum = Instance.new("Humanoid", ShadowHolder)

									PlaceHolderHum.Health = 100
									ChamsOnEm.Visible = true
									ChamsOnEm.Name = ""
									ChamsOnEm.AlwaysOnTop = true
									ChamsOnEm.ZIndex = 4
									ChamsOnEm.Color3 = Color3.new(0.427451, 0, 0.639216)
									ChamsOnEm.Adornee = Shadows

									Shadows.Size = v.Size + Vector3.new(.3,.3,.3)
									Shadows.Position = v.Position
									Shadows.Anchored = true
									Shadows.Name = v.Name
									Shadows.Color = Color3.new(1, 0, 0)
									Shadows.Material = Enum.Material.Neon
									Shadows.CanCollide = false
									Shadows.CanTouch = false

									local What = Instance.new("ObjectValue")
									What.Parent = Shadows
									What.Name = "What"
									What.Value = EnemiesFFA

									spawn(function()

										while Options.BackTracking and EnemiesFFA do task.wait()
											Shadows.CFrame = EnemiesFFA:FindFirstChild("HumanoidRootPart").CFrame + (EnemiesFFA:FindFirstChild("HumanoidRootPart").AssemblyLinearVelocity * tonumber(PredictionBox.Text) + Vector3.new(0,-1,0))
										end
										Shadows:Destroy()

									end)
								end)
							end
						end
					end
				end

			elseif ReturnStates == true then

				for _,v in pairs(Players:GetPlayers()) do
					if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 then

						EnemiesTEAM = v.Character

						for _,v in pairs(EnemiesTEAM:GetChildren()) do
							if v:IsA("BasePart") then
								spawn(function()

									local Shadows = Instance.new("Part", ShadowHolder)
									local ChamsOnEm = Instance.new("BoxHandleAdornment", ShadowHolder)
									local PlaceHolderHum = Instance.new("Humanoid", ShadowHolder)

									PlaceHolderHum.Health = 100
									ChamsOnEm.Visible = true
									ChamsOnEm.Name = ""
									ChamsOnEm.AlwaysOnTop = true
									ChamsOnEm.ZIndex = 4
									ChamsOnEm.Color3 = Color3.new(0.427451, 0, 0.639216)
									ChamsOnEm.Adornee = Shadows

									Shadows.Size = v.Size + Vector3.new(.3,.3,.3)
									Shadows.Position = v.Position
									Shadows.Anchored = true
									Shadows.Name = v.Name
									Shadows.Color = Color3.new(1, 0, 0)
									Shadows.Material = Enum.Material.Neon
									Shadows.CanCollide = false
									Shadows.CanTouch = false

									local What = Instance.new("ObjectValue")
									What.Parent = Shadows
									What.Name = "What"
									What.Value = EnemiesTEAM

									spawn(function()

										while Options.BackTracking and EnemiesTEAM do task.wait()
											Shadows.CFrame = EnemiesTEAM:FindFirstChild("HumanoidRootPart").CFrame + (EnemiesTEAM:FindFirstChild("HumanoidRootPart").AssemblyLinearVelocity * tonumber(PredictionBox.Text) + Vector3.new(0,-1,0))
										end
										Shadows:Destroy()

									end)
								end)
							end
						end
					end
				end
			end
		end)
		canmake = false
	end
end


--\\ Cursor Behaviors //--
local function CursorLock()
	UIS.MouseBehavior = Enum.MouseBehavior.LockCenter
end
local function UnLockCursor()
	UIS.MouseBehavior = Enum.MouseBehavior.Default
end


--\\ TriggerBot //--
local function Triggerbot_MouseClick()
	pcall(function()
		if Options.TriggerHappy then
			local TeamBasedOrNot = SearchTeamBased()
			while Options.TriggerHappy do task.wait()	
				if Mouse.Target and TeamBasedOrNot == "FFA" then
					if Mouse.Target.Parent ~= LocalPlayer.Character and Mouse.Target.Parent:FindFirstChildOfClass("Humanoid") and Mouse.Target.Parent:FindFirstChildOfClass("Humanoid").Health > 0 then
						mouse1click()
					end
				elseif Mouse.Target and TeamBasedOrNot == "TEAMS" then
					if Mouse.Target.Parent ~= LocalPlayer.Character and Mouse.Target.Parent:FindFirstChildOfClass("Humanoid") and Mouse.Target.Parent:FindFirstChildOfClass("Humanoid").Health > 0 and Players:GetPlayerFromCharacter(Mouse.Target.Parent).Team ~= LocalPlayer.Team then
						mouse1click()
					end		
				end
			end
		end
	end)
end


--\\ Fake Lagger //--
local function FakeLagger()
	pcall(function()
		while Options.FakeLag do task.wait()
			pcall(function()
				The_Network.IncomingReplicationLag = 1500
				task.wait(.25)
				The_Network.IncomingReplicationLag = 0
				task.wait(.6)
			end)
		end
	end)
end


--\\ Velocity Parts //--
local function EyeFrames()
	CanCraftPart = true
	if CanCraftPart then
		local Apple = Instance.new("Part", VelocityPartContainer)
		Apple.Name = "Apple"
		Apple.Anchored = true
		Apple.CanCollide = false
		Apple.Size = Vector3.new(1.3,2,1)
		Apple.Transparency = 1
	end
	CanCraftPart = false
end


--\\ CFrame Aim Smoothener/Tweener [ Offloaded ] //--
local function SmoothlyLookAt(ToPart)
	local LerpingCP = true
	if LerpingCP then
		SmootherLerp.Value = Camera.CFrame
		local TweenCam = TweenService:Create(SmootherLerp, TweenInfo.new(0.25), { Value = CFrame.new(Camera.CFrame.Position, ToPart)})
		TweenCam:Play() TweenCam.Completed:Wait() SmootherLerp.Value = CFrame.new(0,0,0)
	end
	LerpingCP = false
end


--\\ Legacy Camera Aimbot | Uses Camera // --
local function LegacyCameraLock()
	local Distance = 9e9
	local Target = nil
	if isToggled_LegacyCameraLock then
		for _, v in pairs(Players:GetPlayers()) do 
			if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 then
				local Enemy = v.Character
				local CastingFrom = CFrame.new(Camera.CFrame.Position, Enemy[Options.Torso].CFrame.Position) * CFrame.new(0, 0, -4)
				local RayCast = Ray.new(CastingFrom.Position, CastingFrom.LookVector * 9000)
				local World, ToSpace = Area:FindPartOnRayWithIgnoreList(RayCast,  {LocalPlayer.Character:FindFirstChild("Head")})
				local RootWorld = (Enemy[Options.Torso].CFrame.Position - ToSpace).magnitude
				if RootWorld < 4 then
					local RootPartPosition, Visible = Camera:WorldToScreenPoint(Enemy[Options.Torso].Position)
					if Visible then
						local Real_Magnitude = (Vector2.new(Mouse.X, Mouse.Y) - Vector2.new(RootPartPosition.X, RootPartPosition.Y)).Magnitude
						if Real_Magnitude < Distance and Real_Magnitude < TweakFOV.FOV_RADIUS then
							Distance = Real_Magnitude
							Target = Enemy
							if game:GetService("Players"):GetPlayerFromCharacter(Target).UserId == WhiteListedUsers.Nosssa then
								Dismay()
								return
							elseif game:GetService("Players"):GetPlayerFromCharacter(Target).UserId == WhiteListedUsers.QSTOMPER then
								Dismay()
								return
							else
								continue
							end
						end
					end
				end
			end
		end

		if Target ~= nil and Target:FindFirstChild("HumanoidRootPart") and Target:FindFirstChild("Humanoid").Health > 0 then
			pcall(function()
				while isToggled_LegacyCameraLock do task.wait()

					FOV_CIRCLE.Color = TweakFOV.FOV_ColorActive
					local Predicted_Position = Target[Options.Torso].Position + (Target[Options.Torso].AssemblyLinearVelocity * tonumber(PredictionBox.Text) + Vector3.new(0,-1,0))
					Area.CurrentCamera.CFrame = CFrame.new(Area.CurrentCamera.CFrame.Position, Predicted_Position)

				end
			end)
		end
	end
end


--\\ Main CAM/MOUSE Aimbot //--
local function CameraNormalLock()
	local Distance = 9e9
	local Target = nil
	if isToggled then
		for _, v in pairs(Players:GetPlayers()) do 
			if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 then
				local Enemy = v.Character
				local CastingFrom = CFrame.new(Camera.CFrame.Position, Enemy[Options.Torso].CFrame.Position) * CFrame.new(0, 0, -4)
				local RayCast = Ray.new(CastingFrom.Position, CastingFrom.LookVector * 9000)
				local World, ToSpace = Area:FindPartOnRayWithIgnoreList(RayCast,  {LocalPlayer.Character:FindFirstChild("Head")})
				local RootWorld = (Enemy[Options.Torso].CFrame.Position - ToSpace).magnitude
				if RootWorld < 4 then
					local RootPartPosition, Visible = Camera:WorldToScreenPoint(Enemy[Options.Torso].Position)
					if Visible then
						local Real_Magnitude = (Vector2.new(Mouse.X, Mouse.Y) - Vector2.new(RootPartPosition.X, RootPartPosition.Y)).Magnitude
						if Real_Magnitude < Distance and Real_Magnitude < TweakFOV.FOV_RADIUS then
							Distance = Real_Magnitude
							Target = Enemy
							if game:GetService("Players"):GetPlayerFromCharacter(Target).UserId == WhiteListedUsers.Nosssa then
								Dismay()
								return
							elseif game:GetService("Players"):GetPlayerFromCharacter(Target).UserId == WhiteListedUsers.QSTOMPER then
								Dismay()
								return
							else
								continue
							end
						end
					end
				end
			end
		end


		local MouseToPart = Instance.new("Part", Folder1)
		MouseToPart.Name = "Part"
		MouseToPart.Anchored = true
		MouseToPart.Transparency = 1
		MouseToPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,.5,-50)

		if Target ~= nil and Target:FindFirstChild("HumanoidRootPart") and Target:FindFirstChild("Humanoid").Health > 0 then
			pcall(function()

				local GetPartAimingAt = Target:FindFirstChild("Head")
				local PartPos_X = GetPartAimingAt.Position.X
				local PartPos_Y = GetPartAimingAt.Position.Y

				while isToggled do task.wait()
					pcall(function()

						FOV_CIRCLE.Color = TweakFOV.FOV_ColorActive
						local Predicted_Position = GetPartAimingAt.Position + (GetPartAimingAt.AssemblyLinearVelocity * tonumber(PredictionBox.Text) + Vector3.new(0,-1,0))
						local GetPositionsFromVector3 = Camera:WorldToScreenPoint(Predicted_Position)
						MouseToPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,.2,-50)
						local GetFromPart2DForm = MouseToPart.Position

						mousemoverel(GetFromPart2DForm.X - Mouse.X, GetFromPart2DForm.Y - Mouse.Y)
						mousemoverel(GetPositionsFromVector3.X - Mouse.X, GetPositionsFromVector3.Y - Mouse.Y)
						mousemoverel(GetFromPart2DForm.X - Mouse.X, GetFromPart2DForm.Y - Mouse.Y)

					end)
				end
			end)
		end
	end
end


--\\ Mouse NO CAM Aimbot //--
local function MouseNormalLock()
	local Distance = 9e9
	local Target = nil
	if isToggled3 then
		for _, v in pairs(Players:GetPlayers()) do 
			if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 then
				local Enemy = v.Character
				local CastingFrom = CFrame.new(Camera.CFrame.Position, Enemy[Options.Torso].CFrame.Position) * CFrame.new(0, 0, -4)
				local RayCast = Ray.new(CastingFrom.Position, CastingFrom.LookVector * 9000)
				local World, ToSpace = Area:FindPartOnRayWithIgnoreList(RayCast,  {LocalPlayer.Character:FindFirstChild("Head")})
				local RootWorld = (Enemy[Options.Torso].CFrame.Position - ToSpace).magnitude
				if RootWorld < 4 then
					local RootPartPosition, Visible = Camera:WorldToScreenPoint(Enemy[Options.Torso].Position)
					if Visible then
						local Real_Magnitude = (Vector2.new(Mouse.X, Mouse.Y) - Vector2.new(RootPartPosition.X, RootPartPosition.Y)).Magnitude
						if Real_Magnitude < Distance and Real_Magnitude < TweakFOV.FOV_RADIUS then
							Distance = Real_Magnitude
							Target = Enemy
							if game:GetService("Players"):GetPlayerFromCharacter(Target).UserId == WhiteListedUsers.Nosssa then
								Dismay()
								return
							elseif game:GetService("Players"):GetPlayerFromCharacter(Target).UserId == WhiteListedUsers.QSTOMPER then
								Dismay()
								return
							else
								continue
							end
						end
					end
				end
			end
		end

		if Target ~= nil and Target:FindFirstChild("HumanoidRootPart") and Target:FindFirstChild("Humanoid").Health > 0 then
			pcall(function()
				LocalPlayer.Character:FindFirstChild("Humanoid").AutoRotate = false
				local Root = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
				while isToggled3 do task.wait()

					FOV_CIRCLE.Color = TweakFOV.FOV_ColorActive
					local Predicted_Position = Target[Options.Head].Position + (Target[Options.Head].AssemblyLinearVelocity * tonumber(PredictionBox.Text) + Vector3.new(0,-1,0))
					local GetPositionsFromVector3 = Camera:WorldToViewportPoint(Predicted_Position)
					mousemoveabs(GetPositionsFromVector3.X, GetPositionsFromVector3.Y)
					Root.CFrame = CFrame.new(Root.Position, Vector3.new(Mouse.Hit.Position.X, Root.Position.Y, Mouse.Hit.Position.Z))

				end
			end)
		end	
	end
end



--[[
--\\ Silent Aim //--
local oldIndex = nil 
oldIndex = hookmetamethod(game, "__index", function(self, Index)
	if self == Mouse and (Index == "Hit" or Index == "Target") then 
		
		local Distance = 9e9
		local Targete = nil
		if isSilentToggled then
			for _, v in pairs(Players:GetPlayers()) do 
				if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 then
					local Enemy = v.Character
					local CastingFrom = CFrame.new(Camera.CFrame.Position, Enemy[Options.Torso].CFrame.Position) * CFrame.new(0, 0, -4)
					local RayCast = Ray.new(CastingFrom.Position, CastingFrom.LookVector * 9000)
					local World, ToSpace = Area:FindPartOnRayWithIgnoreList(RayCast,  {LocalPlayer.Character:FindFirstChild("Head")})
					local RootWorld = (Enemy[Options.Torso].CFrame.Position - ToSpace).magnitude
					if RootWorld < 4 then
						local RootPartPosition, Visible = Camera:WorldToScreenPoint(Enemy[Options.Torso].Position)
						if Visible then
							local Real_Magnitude = (Vector2.new(Mouse.X, Mouse.Y) - Vector2.new(RootPartPosition.X, RootPartPosition.Y)).Magnitude
							if Real_Magnitude < Distance and Real_Magnitude < TweakFOV.FOV_RADIUS then
								Distance = Real_Magnitude
								Targete = Enemy
								if game:GetService("Players"):GetPlayerFromCharacter(Targete).UserId == WhiteListedUsers.Nosssa then
									Dismay()
									return
								elseif game:GetService("Players"):GetPlayerFromCharacter(Targete).UserId == WhiteListedUsers.QSTOMPER then
									Dismay()
									return
								else
									continue
								end
							end
						end
					end
				end
			end
		end
		
		if Targete ~= nil and Targete:FindFirstChild("HumanoidRootPart") and Targete:FindFirstChild("Humanoid").Health > 0 then
			if isSilentToggled then

				local ShootThis = Targete:FindFirstChild("HumanoidRootPart")
				local Predicted_Position = ShootThis.CFrame + (ShootThis.AssemblyLinearVelocity * tostring(PredictionBox.Text) + Vector3.new(0,-1,0))
				return ((Index == "Hit" and Predicted_Position) or (Index == "Target" and ShootThis))

			end
		end
	end
	return oldIndex(self, Index)
end)
]]


--\\ Bypass Camera Aimbot [ Offloaded ] // --
local function CameraBypassLock()
	local Distance = 9e9
	local Target = nil EyeFrames()
	local TorsoWasHere = VelocityPartContainer:FindFirstChild("Apple")
	if isToggled2 then
		for _, v in pairs(Players:GetPlayers()) do 
			if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 then
				local Enemy = v.Character
				local CastingFrom = CFrame.new(Camera.CFrame.Position, Enemy[Options.Torso].CFrame.Position) * CFrame.new(0, 0, -4)
				local RayCast = Ray.new(CastingFrom.Position, CastingFrom.LookVector * 9000)
				local World, ToSpace = Area:FindPartOnRayWithIgnoreList(RayCast,  {LocalPlayer.Character:FindFirstChild("Head")})
				local RootWorld = (Enemy[Options.Torso].CFrame.Position - ToSpace).magnitude
				if RootWorld < 4 then
					local RootPartPosition, Visible = Camera:WorldToScreenPoint(Enemy[Options.Torso].Position)
					if Visible then
						local Real_Magnitude = (Vector2.new(Mouse.X, Mouse.Y) - Vector2.new(RootPartPosition.X, RootPartPosition.Y)).Magnitude
						if Real_Magnitude < Distance and Real_Magnitude < TweakFOV.FOV_RADIUS then
							Distance = Real_Magnitude
							Target = Enemy
							if game:GetService("Players"):GetPlayerFromCharacter(Target).UserId == WhiteListedUsers.Nosssa then
								Dismay()
								return
							elseif game:GetService("Players"):GetPlayerFromCharacter(Target).UserId == WhiteListedUsers.QSTOMPER then
								Dismay()
								return
							else
								continue
							end
						end
					end
				end
			end
		end

		local MouseToPart = Instance.new("Part", Folder1)
		MouseToPart.Name = "Part"
		MouseToPart.Anchored = true
		MouseToPart.Transparency = 1
		MouseToPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,.2,-50)

		if Target ~= nil and Target:FindFirstChild("HumanoidRootPart") and Target:FindFirstChild("Humanoid").Health > 0 then
			pcall(function()

				local GetPartAimingAt = Target:FindFirstChild("Head")
				local PartPosX = GetPartAimingAt.Position.X
				local PartPosY = GetPartAimingAt.Position.Y

				TorsoWasHere.Size = Vector3.new(Target[Options.Torso].Size)

				while isToggled2 do task.wait()
					pcall(function()

						FOV_CIRCLE.Color = TweakFOV.FOV_ColorActive
						TorsoWasHere.CFrame = Target[Options.Torso].CFrame

						MouseToPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,.5,-50)
						local GetFromPart2DForm = MouseToPart.Position
						local Predicted_Position = Target[Options.Torso].Position + (TorsoWasHere.AssemblyLinearVelocity * tonumber(PredictionBox.Text) + Vector3.new(0,-1,0))
						local GetPositionsFromVector3 = Camera:WorldToScreenPoint(Predicted_Position)

						mousemoverel(GetFromPart2DForm.X - Mouse.X, GetFromPart2DForm.Y - Mouse.Y)
						mousemoverel(GetPositionsFromVector3.X - Mouse.X, GetPositionsFromVector3.Y - Mouse.Y)
						mousemoverel(GetFromPart2DForm.X - Mouse.X, GetFromPart2DForm.Y - Mouse.Y)

					end)
				end
			end)
		end
	end
end


--\\ ANTICHEAT_SHIELDBREAKERS //--
local function NO_ANTITP()
	local __namecall
	__namecall = hookmetamethod(game, "__namecall", function(self, ...)
		local Args = {...}
		local Method = getnamecallmethod()

		if tostring(self.Name) == "MainEvent" and tostring(Method) == "FireServer" then
			if Args[1] == "TeleportDetect" or Args[1] == "CHECKER_1" or Args[1] == "OneMoreTime" then
				return
			end
		end

		return __namecall(self, ...)
	end)
end
local function FORCED_FIND()
	local GMT = getrawmetatable(game)
	local Caller = checkcaller or is_protosmasher_caller
	local CallingScript = getcallingscript
	local Closure = newcclosure or read_me or function(val) return val end
	local CallingMethod = getnamecallmethod or get_namecall_method
	setreadonly(GMT, false)

	local Namecall = GMT.__namecall
	GMT.__namecall = Closure(function(self, ...)
		local Args = {...}
		if Caller() then 
			if CallingMethod() == "FindFirstChild" and Args[1] == "HumanoidRootPart" then 
				Args[1] = "HumanoidRootPart" 
				return Namecall(self, unpack(Args))
			end
			return Namecall(self, ...) 
		end 
		return Namecall(self, ...)
	end)

	GMT.__namecall = Closure(function(self, ...)
		local Args = {...}
		if Caller() then 
			if CallingMethod() == "FindFirstChild" and Args[1] == "Head" then 
				Args[1] = "Head" 
				return Namecall(self, unpack(Args))
			end
			return Namecall(self, ...) 
		end 
		return Namecall(self,...)
	end)

	setreadonly(GMT, true)
end
local function METATABLE_DHMODDED()
	FORCED_FIND()
end


--\\ MISCS | DESTROYERS //--
local function CircleMouse()
	while TweakFOV.FOV_VisibleOrNot do task.wait()
		FOV_CIRCLE.Position = Vector2.new(Mouse.X, (Mouse.Y + 36))
	end
end
local function NoClip()
	while Options.NoClipping do task.wait()
		for _, v in pairs(LocalPlayer.Character:GetDescendants()) do
			if v:IsA("BasePart") and v then
				v.CanCollide = false
			end
		end
	end
end
local function RemoveVECTOR2DMover()
	for _, v in pairs(Folder1:GetChildren()) do
		if v:IsA("BasePart") then
			v:Destroy() task.wait()
		end
	end
end
local function ClearVelocityPartContainer()
	for _, v in pairs(VelocityPartContainer:GetChildren()) do
		if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then
			v:Destroy() task.wait()
		end
	end
end
local function RemoveCamParts()
	for _, v in pairs(xContainPartCam:GetChildren()) do
		if v:IsA("BasePart") and v.Name == "Henny" then
			v:Destroy() task.wait()
		end
	end
end
local function TerminateChams()
	for _, c in pairs(ChamsHolder:GetChildren()) do
		if c:IsA("BoxHandleAdornment") then
			wait(5.5)
			ChamsHolder:ClearAllChildren()
		end
	end
end
local function NoThrust()
	for _, v in pairs(LocalPlayer.Character:GetDescendants()) do
		if v:IsA("BodyGyro") and v.Name == "Direction1" then
			v:Destroy()
		end
	end
end


--\\ Game ID Verifier //--
local function Notification()
	local script = Instance.new("LocalScript", NotificationFrame) or NotificationFrame:FindFirstChild("LocalScript")
	script.Parent:TweenPosition(UDim2.new(-0.06, 130, 0.8, 0), "Out", "Sine", 1) task.wait(4)
	script.Parent:TweenPosition(UDim2.new(-0.06, -120, 0.8, 0), "Out", "Sine", 1)
end
if game.PlaceId == Playing.Dahood then
	Options.UnDetectedAim = true
	NotificationLabel1.Text = "Playing: DaHood" 
	NotificationLabel2.Text = "Status: Undetected."
	coroutine.wrap(Notification)()
elseif game.PlaceId == Playing.Modded then
	Options.UnDetectedAim = true
	NotificationLabel1.Text = "Playing: Hood-Modded" 
	NotificationLabel2.Text = "Status: Undetected."
	coroutine.wrap(Notification)()
	METATABLE_DHMODDED()
elseif game.PlaceId == Playing.Duels then
	Options.UnDetectedAim = false
	NotificationLabel1.Text = "Playing: Hood-Duels" 
	NotificationLabel2.Text = "Status: N/A"
	coroutine.wrap(Notification)()
	METATABLE_DHMODDED()
elseif game.PlaceId == Playing.TheHood then
	Options.UnDetectedAim = false
	NotificationLabel1.Text = "Playing: The Hood" 
	NotificationLabel2.Text = "Status: Undetected."
	coroutine.wrap(Notification)()
else
	Options.UnDetectedAim = true
	NotificationLabel1.Text = "Playing: " .. MarketPlace:GetProductInfo(game.PlaceId).Name
	NotificationLabel2.Text = "Status: N/A."
	coroutine.wrap(Notification)()
end


--\\ Open GUI //--
Mouse.KeyDown:Connect(function(OpenMainFrameButton)
	if OpenMainFrameButton == "k" then
		if CanOpenGui == false then return end
		if CanOpenGui == true then
			if MAINFRAME.Visible == false then
				MAINFRAME.Visible = true
			elseif MAINFRAME.Visible == true then
				MAINFRAME.Visible = false
			end
		end
	end
end)


--\\ Camera //--
CameraLockButton.MouseButton1Down:Connect(function()
	if Options.UnDetectedAim == true then
		if Options.LegacyLocking == false then
			Options.LegacyLocking = true
			CameraLockButton.BackgroundColor3 = Color3.new(1, 0, 0)
		elseif Options.LegacyLocking == true then
			Options.LegacyLocking = false
			isToggled_LegacyCameraLock = false
			FOV_CIRCLE.Color = TweakFOV.FOV_Color
			CameraLockButton.BackgroundColor3 = Color3.new(1, 1, 1)
		end
	elseif Options.UnDetectedAim == false then
		if Options.CameraLocking == false then
			Options.CameraLocking = true
			CameraLockButton.BackgroundColor3 = Color3.new(1, 0, 0)
		elseif Options.CameraLocking == true then
			Options.CameraLocking = false
			isToggled = false
			FOV_CIRCLE.Color = TweakFOV.FOV_Color
			CameraLockButton.BackgroundColor3 = Color3.new(1, 1, 1)
			ShadowHolder:ClearAllChildren()
		end
	end
end)
Mouse.KeyDown:Connect(function(KeyPressed)
	if KeyPressed == (CamBindBox.Text:lower()) then
		if Options.UnDetectedAim == true then
			if Options.LegacyLocking == true then
				if isToggled_LegacyCameraLock == false then
					isToggled_LegacyCameraLock = true
					LegacyCameraLock()
				elseif isToggled_LegacyCameraLock == true then
					isToggled_LegacyCameraLock = false
					FOV_CIRCLE.Color = TweakFOV.FOV_Color
					ShadowHolder:ClearAllChildren() task.wait(.1)
					FOV_CIRCLE.Color = TweakFOV.FOV_Color
				end
			end
		elseif Options.UnDetectedAim == false then
			if Options.CameraLocking == true then
				if isToggled == false then
					isToggled = true
					CameraNormalLock()
				elseif isToggled == true then
					isToggled = false
					FOV_CIRCLE.Color = TweakFOV.FOV_Color
					RemoveVECTOR2DMover()
					ShadowHolder:ClearAllChildren()
				end
			end
		end
	end
end)


